    let currentCounter = localStorage.getItem('_vkCheckoutStart');
    if (!currentCounter) {
        _tmr.push({ type: 'reachGoal', id: 3284847, goal: 'checkout_start'});
        localStorage.setItem('_vkCheckoutStart', 1);
        return;
    }
    return;
}

function vkCheckout() {
    let currentCounter = localStorage.getItem('_vkCheckout');
    if (!currentCounter) {
        _tmr.push({ type: 'reachGoal', id: 3284847, goal: 'checkout_end'});
        localStorage.setItem('_vkCheckout', 1);
        return;
    }
    return;
}

function vkPurchase(val){
    let currentCounter = localStorage.getItem('_vkPurchase');
    if (!currentCounter) {
        _tmr.push({ type: 'reachGoal', id: 3284847, value: val, goal: 'purchase'});
        localStorage.setItem('_vkPurchase', 1);
        return;
    }
    return;
}
