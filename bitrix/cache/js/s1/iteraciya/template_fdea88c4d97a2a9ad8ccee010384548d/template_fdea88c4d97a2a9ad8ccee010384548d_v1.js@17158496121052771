function changeVacanciesList(ul_id, title) {
    $('.career__vacancies-list:visible').hide();
    $('#' + ul_id).fadeIn(300);
    $('.career__vacancy-button:visible').text(title);
    $('.career__vacancy-button').click();
}

function setTimlineFilter(obj) {
//var param_name = $(obj).attr('name');
    $(obj).parents('form').submit();
}

function showWaiting() {
    $(overlayer).show();
    wait = BX.showWait('available_action_wrapper');
}

function hideWaiting() {
    BX.closeWait('available_action_wrapper', wait);
    $(overlayer).hide();
}

$(document).ready(function () {
    overlayer = $('#overlayer');
    initJS();


    $(document).on("click", ".to-load-ajax-list", function () {
        var that = $(this);

        if (that.hasClass('disabled'))
            return false;
        that.addClass('disabled');

        let $container = that.parent().find(that.attr("data-container"));

        let curPage = parseInt(that.attr("data-currentpage"));
        let lastPage = parseInt(that.attr("data-lastpage"));
        let navNum = parseInt(that.attr("data-navNum"));

        curPage++;

        let data = 'PAGEN_' + navNum + '=' + curPage + '&AJAX_CATALOG_' + navNum + '=Y';

        $.ajax({
                type: 'get',
                datatype: 'html',
                data: data,
                success: function (result) {
                    $container.append(result);

                    if (curPage >= lastPage) {
                        that.remove();
                    } else {
                        that.removeClass('disabled');
                        that.attr('data-currentpage', curPage);
                    }
                }
            }
        );
        return false;
    });

    $(".blog__news").each(function (index, elem) {
        var el = $(elem);
        if (el.find(".blog__news-wrap").length > 0) {
            el.show();
        }
    });

    $(".club__way-menu li").click(function () {
        var id = $(this).index();

        $(".club__way-menu li").removeClass("active");
        $(this).addClass("active");

        $(".club__way-content-tab").removeClass("active");
        $(".club__way-content-tab:eq(" + id + ")").addClass("active");

        return false;
    });

    if ($(".club__way-menu li").length) {
        $(".club__way-menu li:eq(0)").addClass("active");
        $(".club__way-menu li:eq(0)").click();
    }
});
/* End */
;
; /* Start:"a:4:{s:4:"full";s:96:"/local/templates/iteraciya/components/bitrix/news.list/services_in_main/script.js?16723048212875";s:6:"source";s:81:"/local/templates/iteraciya/components/bitrix/news.list/services_in_main/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(document).ready(function() {
    // Получаем элементы
    var advantagesBlock = document.querySelector('.advantages');
    var advantagesItems = document.querySelectorAll('.advantages__item');
    var advantagesItemClass = '.advantages__item';
    var classIsOpen = 'js-animation';
    if (!advantagesBlock || !advantagesItems.length) {
      return;
    }
    // Закрыть все карточки
    var closeAllItems = function () {
      for (var i = 0; i < advantagesItems.length; i++) {
        advantagesItems[i].classList.remove(classIsOpen);
      }
    };
  
    // Открыть все карточки
    var openAllItems = function () {
      for (var i = 0; i < advantagesItems.length; i++) {
        advantagesItems[i].classList.add(classIsOpen);
      }
    };
  
    // Открыть/закрыть карточку
    var toggleItem = function (element) {
      if (element.classList.contains(classIsOpen)) {
        element.classList.remove(classIsOpen);
      } else {
        closeAllItems();
        element.classList.add(classIsOpen);
      }
    };
  
    // Обработчик событий на документе
    var documentAdvantagesClickHandler = function (evt) {
      if (evt.target.closest(advantagesItemClass)) { // Если клик на карточке преимуществ
        var item = evt.target.closest(advantagesItemClass);
        toggleItem(item);
      }
    };
  
    // Добавляем и убираем состояния и обработчики событий в зависимости от разрешения
    var changeAdvantagesOnResize = function () {
      // Если десктоп или планшет, убираем обработчики событий
      if (window.matchMedia('(min-width: 768px)').matches) {
        document.removeEventListener('click', documentAdvantagesClickHandler);
        // Открываем карточки
        openAllItems();
        // Если мобилка, добавялем обработчики событий
      } else {
        document.addEventListener('click', documentAdvantagesClickHandler);
        // Закрываем карточки
        closeAllItems();
      }
    };
  
    // ПОДКЛЮЧАЕМ ВСЕ
    // Если мобилка, сразу добавить обработчик кликов на весь документ
    if (window.matchMedia('(max-width: 767px)').matches) {
      document.addEventListener('click', documentAdvantagesClickHandler);
      // Иначе просто открываем все карточки
    } else {
      openAllItems();
    }
  
    // Следим, не поменялось ли разрешение
    window.addEventListener('resize', changeAdvantagesOnResize);
});
  
/* End */
;
; /* Start:"a:4:{s:4:"full";s:71:"/local/modules/mx.bitrixcolorproperty/js/jscolor.min.js?166271035129148";s:6:"source";s:55:"/local/modules/mx.bitrixcolorproperty/js/jscolor.min.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
