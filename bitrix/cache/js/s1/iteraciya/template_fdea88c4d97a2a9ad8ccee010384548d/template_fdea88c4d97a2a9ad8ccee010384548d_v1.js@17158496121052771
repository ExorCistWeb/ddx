function sendComagic(name, phone) {
    var credentials = Comagic.getCredentials();
    $.post("/local/ajax/functions.php", {
        action: "comagick",
        credentials: credentials,
        name: name,
        phone: phone
    }, function (response) {
    });
}

var getUrlParameter = function getUrlParameter(sParam) {
    var sPageURL = window.location.search.substring(1),
        sURLVariables = sPageURL.split('&'),
        sParameterName,
        i;

    for (i = 0; i < sURLVariables.length; i++) {
        sParameterName = sURLVariables[i].split('=');

        if (sParameterName[0] === sParam) {
            return typeof sParameterName[1] === undefined ? true : decodeURIComponent(sParameterName[1]);
        }
    }
    return false;
};

function onSubmitFeedback(form) {
    var $form = form;
    $(form).find(".required").each(function () {
        var ec = $(this).attr('data-errorclass');
        if ($(this)[0].id == "fileField"){
            if (!$(this)[0].jFiler?.files) {
                $(this).addClass("error");
                $(this).parent().addClass('callback__text-field--error');
                $(this).parent().addClass('callback__text-field--error_' + ec);
            }
            else {
                $(this).removeClass("error");
                $(this).parent().removeClass('callback__text-field--error');
                $(this).parent().removeClass('callback__text-field--error_' + ec);
                if ($(this).attr('name') == 'PHONE')
                    $(this).parents('.form-group').removeClass('error form-error')
            }
        } else {
            if (!$(this).val().length) {
                $(this).addClass("error");
                $(this).parent().addClass('callback__text-field--error');
                $(this).parent().addClass('callback__text-field--error_' + ec);
                if ($(this).attr('name') == 'PHONE')
                    $(this).parents('.form-group').addClass('error form-error')
            }
            else {
                $(this).parent().removeClass('callback__text-field--error');
                $(this).parent().removeClass('callback__text-field--error_' + ec);
                if ($(this).attr('name') == 'PHONE')
                    $(this).parents('.form-group').removeClass('error form-error')
            }
        }
    });

    if ($(form).find("input[name=AGREE]").length) {
        if (!$(form).find("input[name=AGREE]").prop('checked')) {
            $(form).find("input[name=AGREE]").parent().addClass('error');
        } else {
            $(form).find("input[name=AGREE]").parent().removeClass('error');
        }
    }
//  
    let button = $(form).find('button[type=submit]')
    console.log($(form).find(".error"));
    if (!$(form).find(".error").length) {
        let formName = $(form).find('input[name=FORM_NAME]').val();
        $(button).prop('disabled', true);
        $(button).addClass('loading');
        sessionStorage.setItem('_user_fio', $.trim($(form).find('input[name=FIO]').val()));
        sessionStorage.setItem('_user_phone', $.trim($(form).find('input[name=PHONE]').val()));

        var data = new FormData();
        var name = $('#callback-form-main input[name=FIO]').val(),
            phone = $('#callback-form-main input[name=PHONE]').val();
        data.append('sessid',BX.bitrix_sessid());
        data.append('name',name);
        data.append('email',$('#callback-form-main input[name=EMAIL]').val());
        data.append('FORM_NAME',$('#callback-form-main input[name=FORM_NAME]').val());
        data.append('FORM_ID',$('#callback-form-main input[name=FORM_ID]').val());
        data.append('phone',phone);
        data.append('rim',$('#callback-form-main input[name=RIM]').prop('checked'));
        data.append('tar',$('#callback-form-main input[name=TARIFF]').val());
        data.append('URL',$('#callback-form-main input[name=URL]').val());
        data.append('message',$('#callback-form-main textarea[name=MESSAGE]').val());
        data.append('FORM_TITLE', $('h1').text());
        var url = new URL(location.href);
        var clubId = url.searchParams.get('clubID');
        if (clubId?.length > 0) {
            data.append('CLUB_ID', clubId);
        }
        data.append('UTM_Source',getUrlParameter('utm_source') ?? '');
        data.append('UTM_Medium',getUrlParameter('utm_medium') ?? '');
        data.append('UTM_Campaign',getUrlParameter('utm_campaign') ?? '');
        data.append('UTM_Content',getUrlParameter('utm_content') ?? '');
        data.append('UTM_Term',getUrlParameter('utm_term') ?? '');
        var ajaxURL = '/ajax/sendForm/';
        if ($('#fileField').length && $('#fileField')[0].jFiler?.files){
            data.append('files',$('#fileField')[0].jFiler?.files[0]);
        }
        if (!$(this).parent('form').find(".error").length && !!name && !!phone){
            BX.ajax({
                url: ajaxURL,
                data: data,
                method: 'POST',
                dataType: 'json',
                processData: false,
                preparePost: false,
                onsuccess: function(data){
                  vkLead();
                    $.post("/local/ajax/functions.php", {action: "callback", data: $(form).serialize()}, function (response) {
                        if (response == 'ok') {
                            sendComagic($.trim($(form).find('input[name=FIO]').val()), $.trim($(form).find('input[name=PHONE]').val()));
                            $(button).removeClass('loading');
                            $(button).addClass('success');
                            setTimeout(function(){
                                $(form).find('input[type=text]').val('');
                                $(form).find('input[type=tel]').val('');
                                $(form).find("input[name='CLUB_FROM_ID']").val('');
                                $(form).parents('section').find('.feedback__wrapper--success').fadeIn(300);
                                $(form).parents('section').find('.callback__title-wrapper').hide();
                                $(form).parents('section').find('.callback__form-wrapper').hide();
                                $(form).parents('section').find('.main_feedback__wrapper').hide();
                                $(button).prop('disabled', false);
                            }, 1500);
                        }
                    });
                },
                onfailure: function(){
                    
                }
            });
        }

    }
    // }
}

const phoneFormat = (s, plus = true) => {
    const startsWith = plus ? '7' : '8';
  
    let phone = s.replace(/[^0-9]/g, '');
    if (phone.startsWith('7') && plus) {
      phone = phone.substr(1);
    }
    if (phone.startsWith('8')) {
      phone = phone.substr(1);
    }
  
    return phone.replace(/(\d{3})(\d{3})(\d{2})(\d{2})/g, `${startsWith}$1$2$3$4`);
};

function showFlocktory(el, name, email)
{
    $(el).after(`
        <div class="i-flocktory" data-fl-action="exchange" data-fl-spot="6083" data-fl-user-name="${name}" data-fl-user-email="${email}"></div>
    `);
}

function onSubmitBuy(form) {
    $(form).find(".required").each(function () {
        if (!$(this).val().length) {
            $(this).addClass("error");
            var ec = $(this).attr('data-errorclass');
            $(this).parent().addClass('callback__text-field--error');
            $(this).parent().addClass('callback__text-field--error_' + ec);
        } else {
            $(this).parent().removeClass('callback__text-field--error');
            $(this).parent().removeClass('callback__text-field--error_phone');
            $(this).parent().removeClass('callback__text-field--error_name');
        }
    });
    let rim = $(form).find("input[name=RIM]").prop('checked');
    let button = $(form).find('button[type=submit]')
    if (!$(form).find(".error").length) {
        $(button).prop('disabled', true);
        $(button).addClass('loading');
        let formName = $(form).find('input[name=FORM_NAME]').val();
        sessionStorage.setItem('_user_fio', $.trim($(form).find('input[name=FIO]').val()));
        sessionStorage.setItem('_user_phone', $.trim($(form).find('input[name=PHONE]').val()));
        var ajaxURL = '/ajax/sendForm/';
        var data = new FormData();
        var phone = $('#callback-phonenumber-buy').val();
        data.append('sessid', BX.bitrix_sessid());
        data.append('name', $('#callback-username-buy').val());
        data.append('phone', phone);
        data.append('tar', $('#callback-form-buy input[name=TARIFF]').val());
        data.append('rim', $('#callback-form-buy input[name=RIM]').prop('checked'));
        data.append('FORM_URL', $('#callback-form-buy input[name=URL]').val());
        data.append('FORM_NAME', $('#callback-form-buy input[name=FORM_NAME]').val());
        data.append('FORM_ID', $('#callback-form-buy input[name=RIM]').prop('checked'));
        data.append('UTM_Source', getUrlParameter('utm_source') ?? '');
        data.append('UTM_Medium', getUrlParameter('utm_medium') ?? '');
        data.append('UTM_Campaign', getUrlParameter('utm_campaign') ?? '');
        data.append('UTM_Content', getUrlParameter('utm_content') ?? '');
        data.append('UTM_Term', getUrlParameter('utm_term') ?? '');

        if (!$(this).parent('form').find(".error").length && !!phone){
            BX.ajax({
                url: ajaxURL,
                data: data,
                method: 'POST',
                dataType: 'json',
                processData: false,
                preparePost: false,
                onsuccess: function(data){
                    vkLead();
                    $.post("/local/ajax/functions.php", {action: "callback", data: $(form).serialize()}, function (response) {
                        if (response == 'ok') {
                            sendComagic($.trim($("#callback-username-buy").val()), $.trim($("#callback-phonenumber-buy").val()));
                            $(button).removeClass('loading');
                            $(button).addClass('success');
                            setTimeout(function(){
                                $(button).prop('disabled', false);
                                let url = window.location.href;
                                url = url.split('/')
                                let clubCode = url[4]
                                let tariff = $(form).find("input[name=TARIFF]").val().split(' ')[0];
                                let FIO = $(form).find("input[name=FIO]").val().trim();
                                let phone = $(form).find("input[name=PHONE]").val().replace(/[^0-9]/g,"");
                                sessionStorage['clientFio'] = FIO;
                                sessionStorage['clientPhone'] = phone;
                                hideWaiting();
                                link = '/checkout/?club=' + clubCode + '&tariff=' + tariff + '&FIO=' + FIO + '&phone=' + phone;
                                if (rim == true){
                                    link += '&rim=' + rim;
                                }
                                location.href = link
                            }, 1500);
                        }
                    });
                },
                onfailure: function(){
                        
                }
            });
        }
    }
}

