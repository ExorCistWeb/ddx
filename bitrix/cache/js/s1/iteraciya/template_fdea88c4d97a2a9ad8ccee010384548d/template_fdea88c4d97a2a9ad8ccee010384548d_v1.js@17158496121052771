'use strict';
$(document).ready(function () {

  var body = document.querySelector('body');

  var modal = document.querySelector('.modal-feedback');
  var modalOverlay = document.querySelector('.modal__overlay-feedback');

  var modalText = document.querySelector('.modal-text');
  var modalTextOverlay = document.querySelector('.modal__overlay-text');

  var callbackOpenButtons = document.querySelectorAll('.js-callback-button');
  // var callbackOpenButton = document.querySelector('.js-callback-button');
  var mainCallbackOpenButtons = document.querySelectorAll('.callback-button.js-callback-button');
  var callbackCloseButton = document.querySelector('.callback__close-button');
  var callbackUsername = modal.querySelector('#callback-username');
  var popUp = document.querySelector('.pop-up-block');
  var popUpOverlay = document.querySelector('.overlay');
  var windowPrevenPositionY = 0;

  // Открывает модальное окно
  var openModal = function (cls) {

  	var modal = $(cls)[0];


    if (window.matchMedia('(max-width: 1023px)').matches) {
      windowPrevenPositionY = window.pageYOffset; // На мобильных и планшетах запоминаем положение окна перед открытием модалки, чтобы не скроллило к самому верху
    }
    if (!modal.classList.contains('modal--active')) {
      setTimeout(function () {
        modal.classList.add('modal--active');
      }, 1);
      setTimeout(function () {
        modal.classList.add('js-animation');
      }, 100);
      if (popUpOverlay && popUp && !popUp.classList.contains('visually-hidden')) {
        popUp.classList.add('visually-hidden');
        popUpOverlay.classList.add('visually-hidden');
      }
    }
    if (!body.classList.contains('scroll-hidden')) {
      if (window.matchMedia('(max-width: 1023px)').matches) {
        setTimeout(function () {
          body.classList.add('scroll-hidden');
        }, 400);
      } else {
        body.classList.add('scroll-hidden');
      }
    }
    if (callbackUsername) {
      callbackUsername.focus();
    }
  };



  /*if ($(".feedback__wrapper2").length) {
	  openModal();
	}*/


	if ($(".modal-text").length) {
		openModal(".modal-text");
	}

	$(document).on('click', '.to-open-modal-buy', function(){
    if (sessionStorage.getItem('_user_fio')){
      $("#modal-buy input[name=FIO]").val(sessionStorage.getItem('_user_fio'));
    }
    if (sessionStorage.getItem('_user_phone')) {
      $("#modal-buy input[name=PHONE]").val(sessionStorage.getItem('_user_phone'));
    }
		openModal(".modal-buy");
	});

	$("#callback-form-buy").submit(function(){
		console.log(123);
		return false;
	});


  callbackOpenButtons.forEach(function (button) {
    button.addEventListener('click', function (evt) {
      evt.preventDefault();
      // if (button.classList.contains('callback-button--visible')) {
      //   button.classList.remove('callback-button--visible');
      // }

      mainCallbackOpenButtons.forEach(function (mainCallbackOpenButton) {
        if (mainCallbackOpenButton.classList.contains('callback-button--visible')) {
          mainCallbackOpenButton.classList.remove('callback-button--visible');
        }
      });

      //openModal(evt);

      openModal(".modal-feedback");
    });
  });

  // Закрывает модальное окно
  var closeModal = function (evt, id) {
    evt.preventDefault();

    var modal = $("#"+id)[0];

    if (modal.classList.contains('modal--active')) {
      modal.classList.remove('js-animation');
      setTimeout(function () {
        modal.classList.remove('modal--active');
      }, 300);
    }
    if (body.classList.contains('scroll-hidden')) {
      body.classList.remove('scroll-hidden');
    }
    if (window.matchMedia('(max-width: 1023px)').matches) {
      window.scrollTo(0, windowPrevenPositionY); // Незаметно скроллим к ранее запомненному положению
    }
    // if (!callbackOpenButton.classList.contains('callback-button--visible')) {
    //   callbackOpenButton.classList.add('callback-button--visible');
    // }

    mainCallbackOpenButtons.forEach(function (mainCallbackOpenButton) {
      if (!mainCallbackOpenButton.classList.contains('callback-button--visible')) {
        mainCallbackOpenButton.classList.add('callback-button--visible');
      }
    });
  };

  /*if (callbackCloseButton) {
    callbackCloseButton.addEventListener('click', function (evt) {
      closeModal(evt);
    });
  }*/

  $(".callback__close-button").click(function(evt){
  	var that = $(this);
  	var modal = $(this).parent().parent();
	closeModal(evt, modal.attr('id'));
  	return false;
  });


  /*if (modalOverlay) {
    modalOverlay.addEventListener('click', function (evt) {
      closeModal(evt);
    });
  }*/

  $(".modal__overlay").click(function(evt){
  	var that = $(this);
  	var modal = $(this).parent();
	closeModal(evt, modal.attr('id'));
  	return false;
  });

  /*if (modal) {
    window.addEventListener('keydown', function (evt) {
      if (evt.keyCode === 27) {
        closeModal(evt);
      }
    });
  }*/



  window.addEventListener('keydown', function (evt) {
      if (evt.keyCode === 27) {

      	if ($(".modal.modal--active").length) {
      		closeModal(evt, $(".modal.modal--active").attr('id'));
		}
      }
    });

});

/* eslint-disable one-var */
/* eslint-disable no-empty */
'use strict';
$(document).ready(function () {
  var enter = 13;
  var esc = 27;

  var body = document.querySelector('body');
  var header = document.querySelector('.page-header');
  var headerToggle = header.querySelector('.page-header__menu-toggle');
  var headerLogo = header.querySelector('.page-header__logo a');
  var headerContacts = header.querySelector('.page-header__contacts');
  var itemsHasSublist = document.querySelectorAll('.nav__item-sublist');
  var itemsActive = document.querySelectorAll('.nav__list .is-active a');
  var submenuTogglers = document.querySelectorAll('.nav__submenu-toggle');

  var mainClass = 'main';
  var footerClass = 'footer';
  var headerToggleClass = '.page-header__menu-toggle';
  var classIsOpen = 'page-header--menu-open';
  var itemsHasSublistClass = '.nav__item-sublist';
  var classSublistIsOpen = 'is-open';
  var classSublistIsCurrent = 'is-current';
  var submenuToggleClass = '.nav__submenu-toggle';
  var animationClass = 'js-header-animation';
  var animationSubmenuClass = 'js-sublist-animation';


  if (!body || !header || !headerToggle || !headerLogo || !headerContacts) {
    return;
  }


  // Обработчик событий клавиатуры на документе
  var documentKeyEventHandler = function (evt) {
    if (evt.keyCode === enter || evt.keyCode === esc) {
      if (header.classList.contains(classIsOpen)) {
        toggleMenu();
      }
    }
  };

  // Обработчик событий на документе
  var documentEventHandler = function (evt) {
    if (evt.target.closest(headerToggleClass)) { // Если клик на тогглере меню

      event.preventDefault();
      toggleMenu();
    } else if (evt.target.closest(submenuToggleClass)) { // Если клик на тогглере подменю
      event.preventDefault();
      var target = evt.target.closest(itemsHasSublistClass);
      toggleSubMenu(target);
    } else if (evt.target.closest(mainClass) || evt.target.closest(footerClass)) { // Если клик вне меню
      if (header.classList.contains(classIsOpen)) {
        toggleMenu();
      }
    }
  };

  // Закрыть/открыть меню
  var toggleMenu = function () {
    if (header.classList.contains(classIsOpen)) {
      // Проигрываем анимацию потом удаляем класс
      header.classList.remove(animationClass);
      setTimeout(function () {
        header.classList.remove(classIsOpen);
      }, 300);
      closeAllSubMenu();
      headerToggle.querySelector('span').textContent = 'Открыть меню';
      var currentOffset = body.style.top; // Берет текущий отступ фиксированной страницы
      toggleOverflow();
      window.scrollTo(0, parseInt(currentOffset || '0', 10) * -1); // И скроллит страницу до этого положения
    } else {
      header.classList.add(classIsOpen);
      // Добавляем анимацию с задержкой чтобы проигралась
      setTimeout(function () {
        header.classList.add(animationClass);
      }, 50);
      headerToggle.querySelector('span').textContent = 'Закрыть меню';
      var newOffset = '-' + window.pageYOffset + 'px'; // Фиксирует текущий оффсет и готовит из него значение top для fixed
      toggleOverflow();
      body.style.top = newOffset; // Назначает отступ зафиксированной странице
    }
  };

  // Закрыть/открыть подменю
  var toggleSubMenu = function (target) {
    // Закрываем
    if (target.classList.contains(classSublistIsOpen)) {
      target.classList.remove(animationSubmenuClass);
      setTimeout(function () {
        target.classList.remove(classSublistIsOpen);
      }, 300);
      target.querySelector('button span').textContent = 'Открыть меню';
      // Открываем
    } else {
      target.classList.add(classSublistIsOpen);
      var sublinks = target.querySelectorAll('.nav__subitem');
      for (var i = 0; i < sublinks.length; i++) {
        var delay = 20 * i;
        sublinks[i].style.transitionDelay = delay + 'ms';
      }
      setTimeout(function () {
        target.classList.add(animationSubmenuClass);
      }, 50);
      target.querySelector('button span').textContent = 'Закрыть меню';
    }
  };

  // Закрыть все подменю (кроме случая если мы на странице подменю не на десктопе)
  var closeAllSubMenu = function () {
    if (itemsHasSublist.length) {
      for (var i = 0; i < itemsHasSublist.length; i++) {
        // Если подменю открыто
        if (itemsHasSublist[i].classList.contains(classSublistIsOpen)) {
          // Если мы на десктопе или не на страние из этого подменю
          if (!itemsHasSublist[i].classList.contains(classSublistIsCurrent) || window.matchMedia('(min-width: 1024px)').matches) {
            toggleSubMenu(itemsHasSublist[i]);
          }
        }
      }
    }
  };

  // Изменить таб-индекс
  var setTabindex = function () {
    if (itemsActive.length > 0) {
      for (var k = 0; k < itemsActive.length; k++) {
        itemsActive[k].tabIndex = -1;
      }
    }
    if (window.matchMedia('(max-width: 1023px)').matches) {
      headerLogo.tabIndex = 2;
      headerContacts.tabIndex = 3;
      headerToggle.tabIndex = 4;
      if (submenuTogglers.length > 0) {
        for (var i = 0; i < submenuTogglers.length; i++) {
          submenuTogglers[i].tabIndex = 0;
        }
      }
    } else {
      headerLogo.tabIndex = 0;
      headerContacts.tabIndex = 0;
      headerToggle.tabIndex = 0;
      if (submenuTogglers.length > 0) {
        for (var x = 0; x < submenuTogglers.length; x++) {
          submenuTogglers[x].tabIndex = -1;
        }
      }
    }
  };

  // Запретить/разрешить скролл страницы
  var toggleOverflow = function () {
    body.classList.toggle('scroll-hidden');
  };

  // Добавляем и убираем состояния и обработчики событий в зависимости от разрешения
  var changeOnResize = function () {

  	return;
  	
    // Меняем таб-индекс
    setTabindex();
    // Закрывем меню

    // Если десктоп, убираем обработчики событий
    if (window.matchMedia('(min-width: 1024px)').matches) {
      document.removeEventListener('click', documentEventHandler);
      document.removeEventListener('keydown', documentKeyEventHandler);
      // Закрываем подменю
      closeAllSubMenu();
      // Закрываем меню
      if (header.classList.contains(classIsOpen)) {
        toggleMenu();
      }
      // Если не десктоп, добавялем обработчики событий
    } else {
      document.addEventListener('click', documentEventHandler);
      document.addEventListener('keydown', documentKeyEventHandler);
    }
  };

  // ПОДКЛЮЧАЕМ ВСЕ
  // Если не десктоп, сразу добавить обработчик кликов и нажатий клавиш на весь документ
  if (window.matchMedia('(max-width: 1023px)').matches) {
    document.addEventListener('click', documentEventHandler);
    document.addEventListener('keydown', documentKeyEventHandler);
  }

  // Если мобильный и подменю должно быть открыто, добавляем класс анимации чтобы показыввался контент
  if (window.matchMedia('(max-width: 1023px)').matches) {
    if (itemsHasSublist.length) {
      for (var i = 0; i < itemsHasSublist.length; i++) {
        if (itemsHasSublist[i].classList.contains(classSublistIsOpen) && itemsHasSublist[i].classList.contains(classSublistIsCurrent)) {
          itemsHasSublist[i].classList.add(animationSubmenuClass);
        }
      }
    }
  }

  setTabindex();
  closeAllSubMenu();

  // Следим, не поменялось ли разрешение
  window.addEventListener('resize', changeOnResize);
});

'use strict';
$(document).ready(function () {
  var offerListButtons = document.querySelectorAll('.partnership__offer-type > button');
  var offerList = document.querySelector('.partnership__offer-type > ul');
  var offerListSelector = '.partnership__offer-type > ul';
  var offer = document.querySelector('.partnership__offer-type');

  if (!offerList || !offer) {
    return;
  }

  // Открывает список типов предложений о сотрудничестве
  var openPartnershipList = function (evt) {
    for (var i = 0; i < offerListButtons.length; i++) {
      if (evt.target === offerListButtons[i]) {
        var target = evt.target.closest('.partnership__offer-type');
        target.classList.toggle('partnership__offer-type--is-open');
      }
    }
  };

  if (offerListButtons.length) {
    document.addEventListener('click', function (evt) {
      if (!evt.target.closest(offerListSelector) && offer.classList.contains('partnership__offer-type--is-open')) {
        offer.classList.toggle('partnership__offer-type--is-open');
      } else {
        openPartnershipList(evt);
      }
    });
  }
});

'use strict';
$(document).ready(function () {

  var popUp = document.querySelector('.pop-up-block');
  var links = Array.from(document.querySelectorAll('.js-link-to-pop-up'));
  var overlay = document.querySelector('.overlay');

  if (!popUp || !links || !overlay) {
    return;
  }

  var closeButton = popUp.querySelector('.pop-up-block__close-btn');

  if (!closeButton) {
    return;
  }

  var openPopUp = function () {
    // popUp.classList.add('pop-up-block--opened');
    popUp.classList.remove('visually-hidden');
    setTimeout(function () {
      popUp.classList.add('js-animation');
    }, 1);
    // overlay.classList.add('overlay--active');
    overlay.classList.remove('visually-hidden');
    document.body.classList.add('scroll-hidden');
  };

  var closePopUp = function () {
    if (!popUp.classList.contains('visually-hidden')) {
      // popUp.classList.remove('pop-up-block--opened');
      popUp.classList.add('visually-hidden');
      popUp.classList.remove('js-animation');
      // overlay.classList.remove('overlay--active');
      overlay.classList.add('visually-hidden');
      document.body.classList.remove('scroll-hidden');
    }
  };

  document.addEventListener('click', function (evt) {
    var target = evt.target;

    if (target.classList.contains('js-link-to-pop-up') && !target.classList.contains('pop-up-block--opened')) {
      openPopUp();
    }

    if (target === closeButton || target === overlay) {
      closePopUp();
    }
  });

  window.addEventListener('keydown', function (evt) {
    if (evt.keyCode === 27) {
      evt.preventDefault();
      closePopUp();
    }
  });
});

/* eslint-disable no-undefined */
'use strict';
$(document).ready(function () {
  /* eslint-disable no-undef */
  // eslint-disable-next-line no-unused-vars
  var swiper = document.querySelector('.js-slider-club');
  var clubSwiper;

  if (!swiper) {
    return;
  }

  var destroySwiper = function () {
    clubSwiper.destroy(true, true);
    clubSwiper = undefined;
  };

  var initSwiperS = function () {
    clubSwiper = new Swiper('.js-slider-club', {
      slidersPerView: 1,
      pagination: {
        el: '.js-slider-club-pagination',
        type: 'bullets',
        clickable: true,
      },
    });
  };

  var initSwiperL = function () {
    clubSwiper = new Swiper('.js-slider-club', {
      slidersPerView: 1,
      pagination: {
        el: '.js-slider-club-pagination',
        type: 'bullets',
        clickable: true,
      },
      navigation: {
        nextEl: '.swiper-button-next',
        prevEl: '.swiper-button-prev',
      },
    });
  };

  var changeClubSwiper = function () {
    if (window.matchMedia('(min-width: 1024px)').matches) {
      if (clubSwiper !== undefined) {
        destroySwiper();
      }
      initSwiperL();
    } else {
      if (clubSwiper !== undefined) {
        destroySwiper();
      }
      initSwiperS();
    }
  };

  window.addEventListener('resize', changeClubSwiper);
  changeClubSwiper();
});

'use strict';
$(document).ready(function () {
  /* eslint-disable no-undef */
  // eslint-disable-next-line no-unused-vars

  $(".slider-main").each(function(index, elem){

	  var sliderWrapper = elem;
	  var mainSwiper = new Swiper($(elem).find('.js-main-slider'), {
	    slidersPerView: 1,
	    effect: 'fade',
	    speed: 750,
	    fadeEffect: {crossFade: true},
	    loop: true,
	    autoplay: {
	      delay: 4500,
	      disableOnInteraction: false,
	    },
	    pagination: {
	      el: $(elem).find('.js-main-slider-pagination'),
	      type: 'bullets',
	      clickable: true,
	    },
	    navigation: {
	      nextEl: $(elem).find('.swiper-button-next'),
	      prevEl: $(elem).find('.swiper-button-prev'),
	    },
	  });
	
	  if (sliderWrapper) {

	    mainSwiper.on('slideChange', function () {
	      sliderWrapper.classList.remove('js-animation');
	    });

	    mainSwiper.on('slideChangeTransitionEnd', function () {
	      sliderWrapper.classList.add('js-animation');
	    });

	    mainSwiper.on('touchEnd', function () {
	      sliderWrapper.classList.add('js-animation');
	    });
	  }
	});
});

