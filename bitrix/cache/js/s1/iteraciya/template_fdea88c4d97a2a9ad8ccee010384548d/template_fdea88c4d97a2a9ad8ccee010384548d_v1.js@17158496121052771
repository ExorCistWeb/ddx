    hideWaiting();
    
    });*/
}

function getCoacheData(id) {

    $('.ajax_coach_replace').hide();

    $.post("/local/ajax/functions.php", {action: "getCoacheData", id: id}, function (data) {

        $('.ajax_replace1').html(data.ajax_replace1);
        $('.ajax_replace2').html(data.ajax_replace2);
        if (!$('#button').hasClass('js-callback-button')) {
            $('#button').addClass('js-callback-button');
        }
        if (!$('#button').hasClass('pop-up-block__button-sign-up--coach')) {
            $('#button').addClass('pop-up-block__button-sign-up--coach');
        }
        $('#button').text('Записаться');
        $('#button').attr("onclick", "");
        $('.ajax_coach_replace').show();

        $("#button").removeAttr('data-club-id');

    }, 'json');

}

function getCoacheData2(id, club_id) {

    $('.ajax_coach_replace').hide();

    $.post("/local/ajax/functions.php", {action: "getCoacheData", id: id}, function (data) {

        $('.ajax_replace1').html(data.ajax_replace1);
        $('.ajax_replace2').html(data.ajax_replace2);
        if (!$('#button').hasClass('js-callback-button')) {
            $('#button').addClass('js-callback-button');
        }
        if (!$('#button').hasClass('pop-up-block__button-sign-up--coach')) {
            $('#button').addClass('pop-up-block__button-sign-up--coach');
        }
        $('#button').text('Записаться');
        $('#button').attr("onclick", "");

        $("#button").attr('data-club-id', club_id);

        $('.ajax_coach_replace').show();

    }, 'json');

}

function getServiceData(id) {

    $('.ajax_coach_replace').hide();

    $.post("/local/ajax/functions.php", {action: "getServiceData", id: id}, function (data) {

        $('.ajax_coach_replace').html(data.ajax_coach_replace);
//$('.ajax_replace2').html(data.ajax_replace2);
        if ($('#button').hasClass('js-callback-button')) {
            $('#button').removeClass('js-callback-button');
        }
        if ($('#button').hasClass('pop-up-block__button-sign-up--coach')) {
            $('#button').removeClass('pop-up-block__button-sign-up--coach');
        }
        $('#button').text('Выбрать клуб');
        $('#button').attr("onclick", "window.location.href='/clubs/'; return false;");
        $('.ajax_coach_replace').show();

    }, 'json');

}

