'use strict';
$(document).ready(function () {
  /* eslint-disable no-undef */
  // eslint-disable-next-line no-unused-vars
  var swiper = document.querySelector('.js-slider-news');
  var newsSwiper;

  if (!swiper) {
    return;
  }

  var destroySwiper = function () {
    newsSwiper.destroy(true, true);
    newsSwiper = undefined;
  };

  var initSwiper = function () {
    newsSwiper = new Swiper('.js-slider-news', {
      slidersPerView: 1,
      spaceBetween: 20,
      pagination: {
        el: '.js-slider-news-pagination',
        type: 'bullets',
        clickable: true,
      },
      touchRatio: 0.5,
    });
  };

  var changeNewsSwiper = function () {
    if (window.matchMedia('(min-width: 1024px)').matches) {
      if (newsSwiper !== undefined) {
        destroySwiper();
      }
    } else {
      if (newsSwiper === undefined) {
        initSwiper();
      }
    }
  };

  window.addEventListener('resize', changeNewsSwiper);
  changeNewsSwiper();
});

/* eslint-disable no-undefined */
/* eslint-disable no-undef */
/* eslint-disable no-unused-vars */
'use strict';
$(document).ready(function () {
  var swiper = document.querySelector('.js-slider-plans');
  var plansSwiper;

  if (!swiper) {
    return;
  }

  var destroySwiper = function () {
    plansSwiper.destroy(true, true);
    plansSwiper = undefined;
  };

  var initSwiper = function () {
    plansSwiper = new Swiper('.js-slider-plans', {
      slidesPerView: 'auto',
      centeredSlides: true,
      initialSlide: 1,
      longSwipes: false,
      mousewheel: {
        sensitivity: 100,
      },
      roundLengths: true,
      touchRatio: 0.1,
    });

    plansSwiper.on('slideChange', function () {

      if (plansSwiper) {

        var previousSlide = plansSwiper.slides[plansSwiper.previousIndex];
        var currentSlide = plansSwiper.slides[plansSwiper.realIndex];
        if (previousSlide.classList.contains('js-view-animation')) {
          previousSlide.classList.remove('js-view-animation');
        }
        currentSlide.classList.add('js-animation');
        previousSlide.classList.remove('js-animation');
      }
    });
  };

  var changeSwiper = function () {
    if (window.matchMedia('(min-width: 768px)').matches) {
      if (plansSwiper !== undefined) {
        destroySwiper();
      }
    } else {
      if (plansSwiper === undefined) {
        initSwiper();
      }
    }
  };

  window.addEventListener('resize', changeSwiper);
  changeSwiper();
});

/* eslint-disable no-console */
'use strict';
$(document).ready(function () {
  var header = document.querySelector('.page-header'); // Шапка сайта
  var clubHeader = document.querySelector('.js-club-header'); // Шапка страницы
  var clubMenu = document.querySelector('.js-club-menu'); // Меню страницы
  var additionalOffset = 200; // Поправка чтобы при ручном скролле элемент считался активным чуть раньше
  var clubHeaderLinks = document.querySelectorAll('.club__menu-item a'); // Все якорные ссылки в меню
  var sections = []; // Все блоки, к которым есть сслыки
  var activeClass = 'visible'; // Подсветка ссылки, чей блок сейчас виден

  if (!clubHeaderLinks.length || !header || !clubMenu) {
    return;
  }

  // Получаем блоки, на которые есть ссылки

  var getSections = function () {
    for (var i = 0; i < clubHeaderLinks.length; i++) {
      var link = clubHeaderLinks[i].hash;
      var id = link.substring(1, link.length);
      var elem = document.getElementById(id);
      sections.push(elem);
    }
  };

  // Подсвечиваем активную ссылку

  var hilightLink = function () {
    // Получаем высоту меню
    var verticalOffset = clubHeader.getBoundingClientRect().bottom;
    //  Задаем минимальное положение элемента. когда ссылкаа подсвечивается
    var offset = verticalOffset + additionalOffset;
    // Если какой-то из блоков в зоне видимости
    if (sections[0].getBoundingClientRect().top <= offset && sections[sections.length - 1].getBoundingClientRect().bottom >= offset) {
      // Для каждого блока
      for (var i = 0; i < sections.length; i++) {
        // Получаем координаты
        var top = sections[i]?.getBoundingClientRect().top;
        var bottom = sections[i]?.getBoundingClientRect().bottom;
        // Если виден, подсвечиваем ссылку на него
        if (top <= offset && bottom >= offset) {
          clubHeaderLinks[i].classList.add(activeClass);
        // Если не виден, убираем подсветку
        } else {
          clubHeaderLinks[i].classList.remove(activeClass);
        }
      }
    // Если ничего не видно, подсвечиваем все ссылки
    } else {
      for (var j = 0; j < clubHeaderLinks.length; j++) {
        clubHeaderLinks[j].classList.add(activeClass);
      }
    }
  };

  var toggleListener = function () {
    if (window.matchMedia('(min-width: 1024px)')) {
      window.addEventListener('scroll', hilightLink);
    } else {
      window.removeEventListener('scroll', hilightLink);
    }
  };

  toggleListener();
  getSections();
  window.addEventListener('resize', toggleListener);
});

'use strict';
$(document).ready(function () {
  var selectedTimetables = document.querySelectorAll('.timetable .timetable__content');
  var timetableIndex = 'tt-';
  var timetableCalendarItems = document.querySelectorAll('.timetable__calendar-list label');
  var timetableCalendarItem = '.timetable__calendar-list label';

  if (!selectedTimetables.length || !timetableCalendarItems.length) {
    return;
  }

  var selectTimetableDay = function (evt) {
    if (evt.target.closest(timetableCalendarItem)) {
      for (var i = 0; i < selectedTimetables.length; i++) {
        if (selectedTimetables[i].classList.contains('timetable__content--selected')) {
          selectedTimetables[i].classList.remove('timetable__content--selected');
        }
      }
      for (var j = 0; j < timetableCalendarItems.length; j++) {
        if (timetableCalendarItems[j].classList.contains('selected')) {
          timetableCalendarItems[j].classList.remove('selected');
        }
      }
      
      if (document.querySelector('.timetable-filters__form input[name="day"]')) {
        document.querySelector('.timetable-filters__form input[name="day"]').value = evt.target.closest(timetableCalendarItem).getAttribute('data-day');
      }

      var selectedTabId = timetableIndex + evt.target.closest(timetableCalendarItem).htmlFor;
      if (!document.querySelector('#' + selectedTabId).classList.contains('timetable__content--selected')) {
        document.querySelector('#' + selectedTabId).classList.add('timetable__content--selected');
      }
      if (evt.target.closest(timetableCalendarItem).classList.contains('selected')) {
        evt.target.closest(timetableCalendarItem).classList.remove('selected');
      }
      if (!evt.target.closest(timetableCalendarItem).classList.contains('selected')) {
        evt.target.closest(timetableCalendarItem).classList.add('selected');
      }
    }
  };


  document.addEventListener('click', function (evt) {
    selectTimetableDay(evt);
  });
});


/** iTrack Integration start */
let current_date = new Date,
    param = window.location.search.replace('?', '').split('&');

for (let i = 0; i < param.length; i++) {
  let j = param[i].split('=');

  if (j[0].indexOf('utm') > -1) {
    setCookie("iTrack_" + j[0], j[1], current_date.getFullYear(), current_date.getMonth(), current_date.getDate() + 1, '/');
  }
}

function setCookie (name, value, exp_y, exp_m, exp_d, path, domain, secure) {
  let cookie_string = name + "=" + escape(value);

  if (exp_y) {
    let expires = new Date(exp_y, exp_m, exp_d);
    cookie_string += "; expires=" + expires.toGMTString();
  }

  if (path) {
    cookie_string += "; path=" + escape(path);
  }
  if (domain) {
    cookie_string += "; domain=" + escape(domain);
  }
  if (secure) {
    cookie_string += "; secure";
  }

  document.cookie = cookie_string;
}
/** iTrack Integration end */
