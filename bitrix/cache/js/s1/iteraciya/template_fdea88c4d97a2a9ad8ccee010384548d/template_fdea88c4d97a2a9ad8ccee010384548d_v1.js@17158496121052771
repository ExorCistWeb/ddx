'use strict';
$(document).ready(function () {
  var openVacanciesListButtons = document.querySelectorAll('.career__vacancy-branch > h3');
  var vacanciesListSelector = '.career__vacancy-branch-list';
  var vacanciesBranches = document.querySelectorAll('.career__vacancy-branch');
  var vacancyBranch = document.querySelector('.career__vacancy-branch');

  if (!openVacanciesListButtons || !vacancyBranch || !vacanciesBranches) {
    return;
  }

  var closeAllvacanciesBranches = function () {
    for (var i = 0; i < vacanciesBranches.length; i++) {
      if (vacanciesBranches[i].classList.contains('career__vacancy-branch--is-open')) {
        vacanciesBranches[i].classList.remove('career__vacancy-branch--is-open');
      }
    }
  };

  // Открывает список подразделений с доступными вакансиями
  var openVacanciesList = function (evt) {
    for (var i = 0; i < openVacanciesListButtons.length; i++) {
      if (evt.target === openVacanciesListButtons[i]) {
        var target = evt.target.closest('.career__vacancy-branch');
        target.classList.toggle('career__vacancy-branch--is-open');
      }
    }
  };

  if (openVacanciesListButtons.length) {
    document.addEventListener('click', function (evt) {
      if (!evt.target.closest(vacanciesListSelector) && !evt.target.classList.contains('career__vacancy-button')) {
        closeAllvacanciesBranches();
      }
      openVacanciesList(evt);
    });
  }
});

/* eslint-disable no-console */
'use strict';
$(document).ready(function () {
  var enter = 13;
  var esc = 27;

  var offset = 10; // Величина скролла после которой появляется меню
  var header = document.querySelector('.page-header');
  var clubMenu = document.querySelector('.js-club-menu');
  var clubHeader = document.querySelector('.js-club-header');
  var clubHeaderLinks = document.querySelectorAll('.js-club-menu a');
  var menuToggle = document.querySelector('.js-club-menu-toggle');
  var menuToggleClass = '.js-club-menu-toggle';
  var classIsOpen = 'is-open';
  // Хлебные крошки
  var breadcrumbs = document.querySelectorAll('.breadcrumbs__item a');

  if (!header || !clubHeader || !menuToggle || !clubHeaderLinks.length || !clubMenu) {
    return;
  }

  var verticalOffset = 0;

  // Изменить таб-индекс
  var setTabindex = function (scrolled) {
    // Если не проскроллено
    if (scrolled === false) {
      // Хлебные крошки кликабельны
      if (breadcrumbs.length > 0) {
        for (var i = 0; i < breadcrumbs.length; i++) {
          breadcrumbs[i].tabIndex = 0;
        }
      }
      // Переключатель меню не кликабелен
      if (menuToggle) {
        menuToggle.tabIndex = -1;
      }
    // Если проскроллено
    } else if (scrolled === true) {
      // Хлебные крошки не кликабельны
      if (breadcrumbs.length > 0) {
        for (var k = 0; k < breadcrumbs.length; k++) {
          breadcrumbs[k].tabIndex = -1;
        }
      }
      // Переключатель меню кликабелен
      if (menuToggle) {
        menuToggle.tabIndex = 0;
      }
    }
  };

  // Кликаем по ссылке и скролим
  var handleAnchorClick = function () {

    // Получаем целевой элемент
    var linkTarget = event.currentTarget.getAttribute('href');

    if (linkTarget.indexOf('#') > -1)
    	event.preventDefault();

    var id = linkTarget.substring(1, linkTarget.length);
    var aim = document.getElementById(id);
    // Вычисляем его координаты
    var bodyRect = document.body.getBoundingClientRect().top;
    var aimRect = aim.getBoundingClientRect().top;
    var aimPosition = aimRect - bodyRect;
    // Находим куда скроллить: координаты элемента минус высота шапки
    var aimOffsetPosition = aimPosition - verticalOffset;
    // И скроллим
    window.scrollTo({top: aimOffsetPosition, behavior: 'smooth'});
  };

  if (clubHeaderLinks) {
    for (var i = 0; i < clubHeaderLinks.length; i++) {
      var link = clubHeaderLinks[i];
      link.addEventListener('click', handleAnchorClick);
    }
  }

  // Закрыть/открыть меню
  var toggleClassMenu = function () {
    if (clubHeader.classList.contains(classIsOpen)) {
      clubHeader.classList.remove(classIsOpen);
      menuToggle.querySelector('span').textContent = 'Открыть меню';
    } else {
      clubHeader.classList.add(classIsOpen);
      menuToggle.querySelector('span').textContent = 'Закрыть меню';
    }
  };

  // Добавить обработчик кликов и нажатий клавиш на весь документ
  document.addEventListener('click', function (evt) {
    documentClubEventHandler(evt);
  });

  document.addEventListener('keydown', function (evt) {
    if (evt.keyCode === enter || evt.keyCode === esc) {
      if (clubHeader.classList.contains(classIsOpen)) {
        toggleClassMenu();
      }
    }
  });

  // Обработчик событий на документе
  var documentClubEventHandler = function (evt) {
    if (evt.target.closest(menuToggleClass)) { // Если клик на тогглере меню
      event.preventDefault();
      toggleClassMenu();
    } else if (clubHeader.classList.contains(classIsOpen)) { // Если клик где угодно еще
      toggleClassMenu();
    }
  };

  var watchScroll = function () {
    if (window.pageYOffset > offset) {
      setTabindex(true);
      // Получаем высоту шапки
      verticalOffset = clubHeader.getBoundingClientRect().bottom;
    } else {
      setTabindex(false);
      if (clubHeader.classList.contains(classIsOpen)) {
        toggleClassMenu();
      }
    }
  };

  window.addEventListener('scroll', watchScroll);
});

'use strict';
$(document).ready(function () {
  var faqItems = document.querySelectorAll('.faq ul li');
  var faqItem = 'li';
  var question = '.faq__question';
  var itemOpenClass = 'is-open';

  // Запускает аккордеон при раскрытии пунктов списка FAQ

  var toggleFaqItem = function (element) {
    if (element.classList.contains(itemOpenClass)) {
      element.classList.remove(itemOpenClass);
    } else {
      closeAllFaqItems();
      element.classList.add(itemOpenClass);
    }
  };

  var closeAllFaqItems = function () {
    for (var i = 0; i < faqItems.length; i++) {
      faqItems[i].classList.remove(itemOpenClass);
    }
  };

  document.addEventListener('click', function (evt) {
    documentFaqClickHandler(evt);
  });

  var documentFaqClickHandler = function (evt) {
    if (evt.target.closest(question) && window.matchMedia('(max-width: 1023px)').matches) {
      var target = evt.target.closest(faqItem);
      toggleFaqItem(target);

      //console.log(target);

      if ($(window).width() < 1024) {
	      $("html,body").stop().animate({scrollTop: $(target).offset().top - $("header").outerHeight()}, 400);
	  }
    }
  };
});

'use strict';
$(document).ready(function () {

  var phoneNumbers = document.querySelectorAll('input[type="tel"]');
  var forms = document.querySelectorAll('.callback form');
  var phoneNumberLength = 16;
  var inputWrapper = '.callback__text-field';
  var inputWrapperError = 'callback__text-field--error';

  // Выбирает все поля для ввода номера и присваивает им маску телефонного номера, а также убирает класс ошибки валидации при верном введении номера
  var phoneNumberValidate = function () {
    /*phoneNumbers.forEach(function (phoneNumber) {
      IMask(phoneNumber, {
        mask: '+{7} 000 000 00 00'
      });
      phoneNumber.addEventListener('input', function () {
        if (phoneNumber.value.length === phoneNumberLength && phoneNumber.closest(inputWrapper).classList.contains(inputWrapperError)) {
          phoneNumber.closest(inputWrapper).classList.remove(inputWrapperError);
        }
      });
    });*/

    phoneNumbers.forEach(function (phoneNumber) {
	    $(phoneNumber).mask('+7 (999) 999-99-99', {placeholder: '+7 ( )'});
	});
  };

  $(document).ready(function(){
	  if (phoneNumbers.length) {
	    phoneNumberValidate();
	  }
  });

  // Отменяет отправку формы при недостаточной длине поля ввода номера и добавляет соответствующее предупреждение
  var submitFormReset = function () {
    forms.forEach(function (form) {
      if ($(form).attr('id') != 'callback-form-buy') {
	      form.addEventListener('submit', function (evt) {
	        /*var currentTelInput = form.querySelector('input[type="tel"]');
	        if (currentTelInput.value.length !== phoneNumberLength) {
	          evt.preventDefault();
	          if (!currentTelInput.closest(inputWrapper).classList.contains(inputWrapperError)) {
	            currentTelInput.closest(inputWrapper).classList.add(inputWrapperError);
	          }
	        } */
	      });
		}
    });
  };

  if (forms.length && phoneNumbers.length) {
    submitFormReset();
  }


  // Проверяет заполнение полей и меняет цвет кнопки отправки в случае их валидности
  var checkInputEmptyValue = function (form) {
    var formButton = form.querySelector('button[type="submit"]');
    var currentInputs = form.querySelectorAll('input[type="text"], input[type="tel"], input[type="email"], input[type="number"], input[type="checkbox"], textarea');
    var currentTelInput = form.querySelector('input[type="tel"]');
    var consentCheckBox = form.querySelector('input[type="checkbox"]');

    var checkTextInputEmptyValue = function () {
      var emptyLengthArray = [];
      if (currentInputs.length) {
        for (var i = 0; i < currentInputs.length; i++) {
          if (currentInputs[i].getAttribute('name') == "RIM"){
            if (currentInputs[i].value.length === 0 || ((currentInputs[i] === currentTelInput) && (currentInputs[i].value.length !== 16)) || !consentCheckBox.checked) {
              emptyLengthArray.push(currentInputs[i].value.length);
            }
          }
        }
        if (emptyLengthArray.length) {
          return false;
        } else {
          return true;
        }
      }
      return '';
    };

    if (currentInputs.length) {
      for (var i = 0; i < currentInputs.length; i++) {

        currentInputs[i].addEventListener('input', function () {

          var inputValid = checkTextInputEmptyValue();
          if (inputValid) {
            if (!formButton.classList.contains('valid')) {
              formButton.classList.add('valid');
            }
          } else if (!inputValid) {
            if (formButton.classList.contains('valid')) {
              formButton.classList.remove('valid');
            }
          }
        });
      }
    }
  };

  if (forms.length) {
    forms.forEach(function (form) {
      checkInputEmptyValue(form);
    });
  }
});

'use strict';
$(document).ready(function () {

  var filtersLink = document.querySelector('.js-filter-button');
  var programLink = document.querySelector('.pop-up-filters__program-link');
  var popUpFilter = document.querySelector('.pop-up-filters');
  var popUpInnerFilter = document.querySelector('.pop-up-filters-inner');
  var filtesButtonClose = document.querySelector('.pop-up-filters__close-button');
  var innerBackLink = document.querySelector('.pop-up-filters-inner__backlink');
  var timetableFiltersItems = document.querySelectorAll('.timetable-filters__item');
  var subfilters = document.querySelectorAll('.timetable-filters__subfilters');

  if (!filtersLink || !programLink || !popUpFilter || !popUpInnerFilter || !filtesButtonClose || !innerBackLink) {
    return;
  }

  filtersLink.addEventListener('click', function () {
    popUpFilter.classList.add('pop-up-filters--opened');
    setTimeout(function () {
      popUpFilter.classList.add('js-animation');
    }, 1);
    document.body.classList.add('scroll-hidden');
  });

  filtesButtonClose.addEventListener('click', function () {
    popUpFilter.classList.remove('pop-up-filters--opened');
    popUpFilter.classList.remove('js-animation');
    document.body.classList.remove('scroll-hidden');
  });

  programLink.addEventListener('click', function () {
    popUpFilter.classList.remove('pop-up-filters--opened');
    popUpFilter.classList.remove('js-animation');
    popUpInnerFilter.classList.add('pop-up-filters-inner--opened');
  });

  innerBackLink.addEventListener('click', function () {
    popUpInnerFilter.classList.remove('pop-up-filters-inner--opened');
    popUpFilter.classList.add('pop-up-filters--opened');
    popUpFilter.classList.add('js-animation');
  });

  window.addEventListener('keydown', function (evt) {
    if (evt.keyCode === 27) {
      evt.preventDefault();
      if (popUpFilter.classList.contains('pop-up-filters--opened')) {
        popUpFilter.classList.remove('pop-up-filters--opened');
        popUpFilter.classList.remove('js-animation');
        document.body.classList.remove('scroll-hidden');
      }

      if (popUpInnerFilter.classList.contains('pop-up-filters-inner--opened')) {
        popUpInnerFilter.classList.remove('pop-up-filters-inner--opened');
        document.body.classList.remove('scroll-hidden');
      }
    }
  });

  timetableFiltersItems.forEach(function (element, index) {
    element.addEventListener('click', function () {
      subfilters[index].classList.toggle('timetable-filters__subfilters--active');
      element.classList.toggle('js-filters-opened');
    });
  });

});

'use strict';
$(document).ready(function () {
  var offset = 10; // Величина скролла после которой видоизменяется меню
  var body = document.querySelector('body');
  var classScroll = 'is-scrolled';


  var watchPageScroll = function () {
    if (window.pageYOffset > offset) {
      body.classList.add(classScroll);
    } else {
      body.classList.remove(classScroll);
    }
  };

  window.addEventListener('scroll', watchPageScroll);
});
