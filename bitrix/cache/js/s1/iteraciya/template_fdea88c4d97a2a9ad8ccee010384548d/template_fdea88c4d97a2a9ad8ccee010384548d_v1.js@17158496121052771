var wait;
var overlayer;

var mh = 0;

function randomInt(min, max) {
    return min + Math.floor((max - min) * Math.random());
}


function submitDonateForm(form) {
    var that = $(form);

    that.find(".required").each(function () {
        if (!$(this).val().length) {
            $(this).addClass("error");

            var ec = $(this).attr('data-errorclass');
            $(this).parent().addClass('callback__text-field--error');
            $(this).parent().addClass('callback__text-field--error_' + ec);
        } else {
            $(this).parent().removeClass('callback__text-field--error');
            $(this).parent().removeClass('callback__text-field--error_phone');
            $(this).parent().removeClass('callback__text-field--error_name');
        }
    });

    if (!that.find(".error").length) {
        var name = $.trim($("#donate_name").val());
        var phone = $.trim($("#donate_phone").val());
        var comment = $.trim($("#donate_comment").val());
        var summ = parseInt($.trim($("#donate_summ").val())) || 0;

        var publicId = that.attr('data-public-id');

    }
}

function initJS() {

    $("input[type=text], input[type=email], input[type=phone], input[type=password], input[type=tel], textarea").keypress(function () {
        $(this).removeClass('error');
    });

    $("input[type=text], input[type=email], input[type=phone], input[type=password], input[type=tel], textarea").change(function () {
        $(this).removeClass('error');
    });

    $("input[name=AGREE]").change(function () {
        if (!$(this).prop('checked')) {
            $(this).addClass('error');
        } else {
            $(this).removeClass('error');
        }
    });

    $(window).resize(function () {
        mh = $(window).height();
    });

    $(window).resize();

    /*$(".club-gallery-a").fancybox({
    prevEffect	: 'none',
    nextEffect	: 'none',
    padding: 'none',
    topRatio: '0.5',
    maxHeight: mh*3/4,
