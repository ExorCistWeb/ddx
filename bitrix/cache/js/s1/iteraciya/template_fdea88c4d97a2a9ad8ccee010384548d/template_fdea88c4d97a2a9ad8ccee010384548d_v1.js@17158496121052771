    });
  }*/
  if (clubAmentiesBlock) {
    visible(clubAmentiesBlock, 'js-animation', 0.7, 1);
  }
  if (coachesPreviewBlock) {
    visible(coachesPreviewBlock, 'js-animation', 0.7, 0.9);
  }
