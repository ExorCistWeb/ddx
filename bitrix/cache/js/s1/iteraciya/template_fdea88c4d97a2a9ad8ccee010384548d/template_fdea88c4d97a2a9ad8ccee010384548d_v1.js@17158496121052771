    let currentCounter = localStorage.getItem('_vkViewForm');
    if (!currentCounter) {
        _tmr.push({ type: 'reachGoal', id: 3284847, goal: 'view_form'});
        localStorage.setItem('_vkViewForm', 1);
        return;
    }
    return;
}

