function sleep(ms) {
    return new Promise(resolve => setTimeout(resolve, ms));
}

function initPhoneMask(el) {
    var $el = $(el);

    var inp = $el.find(".js-phone-input-new-mask");

    var completed = false;

    var maskObj = IMask(inp[0], {
        mask: '+7 (000) 000-00-00',
    });

    inp.keyup(function () {

        if (inp.attr('data-ph-mask') == '+7 (000) 000-00-00') {

            let val = inp.val();

            if (val.substr(0, 6) == '+7 (89' || val.substr(0, 5) == '+7 (7') {

                val = '+7 (9';
                //val = '(9';
                inp.val(val);
                maskObj.updateValue();
            }
        }
    });

    $(document).click(function (event) {
        var $target = $(event.target);
        if (!$target.closest('.form-control-field-phone').length && $el.find('.select-country').hasClass("active")) {
            $el.find('.select-country').removeClass("active");
        }
    });

    inp.change(function () {
        completed = false;
        maskObj.updateValue();
    });

    maskObj.on("complete", function () {
        $el.find(".form-group").removeClass("error");
        $el.find(".form-group").removeClass("form-error");
    });

    inp.on("blur", function () {

        if (!maskObj.masked.isComplete) {
            $el.find(".form-group").addClass("error");
            $el.find(".form-group").addClass("form-error");
        } else {
            $el.find(".form-group").removeClass("error");
            $el.find(".form-group").removeClass("form-error");
        }
    });

    inp.on('paste', function (e) {
        console.log(e, 'paste');
        if (inp.attr('data-ph-mask') == '+7 (000) 000-00-00') {

            let text = $.trim(e?.originalEvent?.clipboardData.getData('text'));
            if (text.substr(0, 1) == '+')
                text = text.substr(1);

            text = $.trim(text);

            if (text.substr(0, 1) == '7' || text.substr(0, 1) == '8')
                text = text.substr(1);


            maskObj.unmaskedValue = text;
            maskObj.updateValue();


            return false;
        }
    });

    $el.find(".select-country-btn").click(function () {
        $el.find(".select-country").toggleClass("active");
    });

    $el.find(".select-country .select-country-list li").click(function () {
        var that = $(this);
        var mask = that.attr('data-ph-mask');
        var plch = that.attr('data-placeholder');

        inp.val('');
        inp.attr('placeholder', plch);

        maskObj.updateValue();
        maskObj.updateOptions({mask: mask});

        $el.find(".select-country .select-country-list li").removeClass("active");
        that.addClass("active");

        $el.find(".select-country-btn .flag").css("background-image", that.find(".flag").css("background-image"));

        $el.find(".select-country").removeClass("active");
        $el.find(".select-country .selected").removeClass("active");
    });

    $el.find(".select-country .selected").click(function () {
        var that = $(this);
        var mask = that.attr('data-ph-mask');
        var plch = that.attr('data-placeholder');

        inp.val('');
        inp.attr('placeholder', plch);

        maskObj.updateValue();
        maskObj.updateOptions({mask: mask});

        $el.find(".select-country .select-country-list li").removeClass("active");
        that.addClass("active");

        $el.find(".select-country-btn .flag").css("background-image", that.find(".flag").css("background-image"));

        $el.find(".select-country").removeClass("active");
    });
}

function number_format(number, decimals, dec_point, thousands_sep) {

    var n = !isFinite(+number) ? 0 : +number,
        prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
        sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
        dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
        toFixedFix = function (n, prec) {
            // Fix for IE parseFloat(0.55).toFixed(0) = 0;
            var k = Math.pow(10, prec);
            return Math.round(n * k) / k;
        },
        s = (prec ? toFixedFix(n, prec) : Math.round(n)).toString().split('.');
    if (s[0].length > 3) {
        s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
    }
    if ((s[1] || '').length < prec) {
        s[1] = s[1] || '';
        s[1] += new Array(prec - s[1].length + 1).join('0');
    }
    return s.join(dec);
}

function showErrorMessage(data) {
    $(".payment-error-message").html(data);
    $(".payment.payment-error").show();
    $(".payment.payment-process").hide();
    $(".payment.payment-step2").hide();
}

function expireTimer(date) {
    // Set the date we're counting down to
    var countDownDate = new Date(date.replace(/-/g, "/")).getTime();
    // Update the count down every 1 second
    var x = setInterval(function () {
        // Get todays date and time
        var now = new Date().getTime();
        // Find the distance between now an the count down date
        var distance = countDownDate - now;
        // Time calculations for days, hours, minutes and seconds
        var days = Math.floor(distance / (1000 * 60 * 60 * 24));
        var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
        var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
        var seconds = Math.floor((distance % (1000 * 60)) / 1000);
        // Output the result in an element with id="demo"
        document.getElementById("expire_timer").innerHTML = "<b>" + days + "</b> дней <b>" + hours + "</b> часов <b>" + minutes + "</b> минут <b>" + seconds + "</b>  секунд";
        // If the count down is over, write some text
        if (distance < 0) {
            clearInterval(x);
            document.getElementById("expire_timer").innerHTML = "EXPIRED";
            $(".error-promocode-text").show()
            $(".payment__subscribe").hide()
            $(".to-checkout-step3").attr("disabled", true);
        } else {
            // $(".error-promocode-text").hide()
            // $(".payment__subscribe").show()
            $(".to-checkout-step3").attr("disabled", false);
        }
    }, 1000);
}

function simulateContract(
    clubId,
    paymentPlanId,
    startDate,
    discount,
    promocode)
{
    var request = $.ajax({
        url: '/ajax/simulateContract',
        type: 'POST',
        dataType: 'json',
        data: {
            clubId: clubId,
            paymentPlanId: paymentPlanId,
            startDate: startDate,
            discountId: discount.id
        }
    })

    request.done(function (res) {
        $("#discountId").val(discount.id);
        $("#joiningFeeByPromocode").val(res.joiningFee);
        $("#promocode-error").hide();
        $(".payment__subscribe").show();
        $("#checkout_code").parent().removeClass("form-error");
        if (promocode != "DDX"){
            $('.payment__subscribe-bot').css('display', 'block');
            $("#checkout_code").parent().addClass("form-success");
            expireTimer(discount.expire_at)
        }
    });

    request.fail(function(res) {
        console.log(res, "error")
    })

}

function getDiscount(promocode, memberStatusId, tariffId) {
    var clubId = $("#checkout_club").data('clubid');
    var startDate = moment(new Date()).format('YYYY-MM-DD\THH:MM:SS');
    var request = $.ajax({
        url: "https://online-buy.ddxfitness.ru/api/discount-by-promocode",
        type: 'GET',
        dataType: 'json',
        contentType: 'application/json',
        global: false,
        data: {
            promocode: promocode,
            memberStatusId: memberStatusId,
            paymentPlanId: tariffId,
            clubId: clubId
        }
    });

    request.done(function(res) {
        simulateContract(clubId, tariffId, startDate, res.data.discount, promocode);
    });

    request.fail(function(jqXHR, textStatus) {
        let err = JSON.parse(jqXHR.responseText)
        $('#promocode-error').html(err.exception.message);
        $("#promocode-error").show();
        $(".payment__subscribe").hide();
        $("#checkout_code").parent().removeClass("form-success");
        $("#checkout_code").parent().addClass("form-error");
        $('.payment__subscribe-bot').css('display', 'none');
        setTimeout(function () {
            $("#checkout_code").val('')
            $("#checkout_code").data('promocode', 'DDX')
            recalcCheckout()
        }, 2000);
    })

}

function recalcCheckout() {
    
    var sum = 0, sum2, sum2orig = 3000, sum3orig;
    //var discount = 500;
    // var promocodeForSum = $("#checkout_code").data('promocode');

    // if (tarid == 'guest') {
    //     $(".tarif_name_1").html("Гостевой визит");
    //     $("#joinFee").hide();
    //     $("#info_payment").html("Гостевой визит");
    //     $("#tarif_price_guest").hide()
    //     $("#procode").hide()
    //     sum = 1000;
    //     sum2 = 0
    //     //discount = 0
    //     $('#checkout_tarif').attr('data-id', 14)
    // } else
    if (tarid == 'infinity') {
        $(".tarif_name_1").html("Infinity");
        // $("#joinFee").show();
        // $("#tarif_price_guest").show();
        // $("#procode").show()
        // $("#info_payment").html("Ежемесячное продление подписки далее будет равно стоимости тарифа")
        sum = 2300;
        $('#checkout_tarif').attr('data-id', 18)
    } else if (tarid == 'light') {
        $(".tarif_name_1").html("Light");
        // $("#joinFee").show();
        // $("#tarif_price_guest").show();
        // $("#procode").show()
        // $("#info_payment").html("Ежемесячное продление подписки далее будет равно стоимости тарифа")
        sum = 1700;
        $('#checkout_tarif').attr('data-id', 20)
    } else if (tarid == 'smart') {
        $(".tarif_name_1").html("Smart");
        // $("#joinFee").show();
        // $("#tarif_price_guest").show();
        // $("#procode").show()
        // $("#info_payment").html("Ежемесячное продление подписки далее будет равно стоимости тарифа")
        sum = 2000;
        $('#checkout_tarif').attr('data-id', 22)
    }

    getDiscount($("#checkout_code").data('promocode'), status, $('#checkout_tarif').attr('data-id'))

    // if (promocodeForSum == "DDX"){
    //     sum2 = 1500
    // } else if (promocodeForSum == "RECOVERY1000"){
    //     sum2 = 1000
    // } else if (promocodeForSum == "RECOVERY2000"){
    //     sum2 = 2000
    // }else if (promocodeForSum == "DDXLOVE"){
    //     sum2 = 1500
    // }else if (promocodeForSum = "SOFIAPRESALE"){
    //     sum2 = 1000
    // }else if (promocodeForSum = "SHUKAPRESALE"){
    //     sum2 = 1500
    // }

    // if($('#checkout_tarif').val() == 'guest'){
    //     getDiscount('', status, $('#checkout_tarif').attr('data-id'))
    // }else{
    // }
    setTimeout(() => {
        sum2 = parseInt($("#joiningFeeByPromocode").val());
        let sum3 = sum2 + sum;
        sum3orig = sum2orig + sum;
        $(".tarif_price_1").html(number_format(sum, 0, '.', ' ') + " Р");
        $(".tarif_price_2").html("<span>" + number_format(sum2orig, 0, '.', ' ') + " Р</span> " + number_format(sum2, 0, '.', ' ') + " Р");
        if (tarid == 'guest') {
            $(".tarif_price_3").html(number_format(sum, 0, '.', ' ') + " Р");
        }
        else{
            $(".tarif_price_3").html("<span>" + number_format(sum3orig, 0, '.', ' ') + " Р</span> " + number_format(sum3, 0, '.', ' ') + " Р");
        }
    }, 1000);
    return false;
}

function checkPromocode() {
    promo = $("#checkout_code").val()
    if (promo.length < 1) {
        $("#checkout_code").parent().removeClass("form-error");
        $("#checkout_code").parent().removeClass("form-success");
        $("#checkout_code").val('')
        $("#checkout_code").data('promocode', 'DDX')
        recalcCheckout()
    } else {
        $("#checkout_code").parent().removeClass("form-error");
        $("#checkout_code").data("promocode", promo);
        if ($("#checkout_tarif").val() !== ''){
            recalcCheckout()
            // getDiscount($("#checkout_code").data('promocode'), status, $('#checkout_tarif').attr('data-id'))
        }else{
            $("#checkout_tarif").parent().addClass("form-error");
        }
    }
}

function getClubHeaderHeight(){
    var ptClud = $('.club__header').outerHeight();
    $('.club').css('padding-top', ptClud + 25);
}

window.addEventListener('resize', function(event){
    getClubHeaderHeight();
});


$(document).ready(function () {

    getClubHeaderHeight();

    $('.club__way-menu').on('click', 'li:not(.active)', function () {
        $(this)
            .addClass('active').siblings().removeClass('active')
            .closest('.club__way').find('.club__way-content-tab').removeClass('active').eq($(this).index()).addClass('active');
    });

    $('.nav__sublist').wrapInner('<div>');
    $('.nav__sublist > div').addClass('scrollbar-inner');
    
    if ($('.scrollbar-inner').length)
        $('.scrollbar-inner').scrollbar();

    if ($(".form-control-field-phone").length) {
        $(".form-control-field-phone").each(function (index, elem) {
            initPhoneMask(elem);
        });
    }

    $('.input-group.date').datepicker({
        format: "dd/mm/yyyy",
        weekStart: 1,
        maxViewMode: 2,
        language: "ru",
        orientation: "bottom auto",
        keyboardNavigation: false,
        autoclose: true,
        todayHighlight: true,
        zIndexOffset: 100,
    }).on('show', function(e) {
        $('.datepicker-switch').on('click', function(){
            event.stopPropagation();
            event.preventDefault();
        })
    }).on('changeDate', function(e){
        recalcCheckout();
    })

    $('#modal-log-check').on('hidden.bs.modal', function (event) {
        location.href = '/';
    })

    $(".to-check-user-yes").click(function () {
        $("#modal-log-check").modal("hide");
        $("#modal-log-check-yes").modal("show");
        setTimeout(function () {
            $("#modal-log-check-yes").modal("hide");
        }, 2000);
        return false;
    });

    $(".to-check-user-no").click(function () {
        $("#modal-log-check").modal("hide");
        setTimeout(function () {
            showErrorMessage("Что то пошло не так...")
        }, 2000);
        return false;
    });

    $('#callback-form-main input[name=EMAIL]').inputmask({ alias: "email"});

    if (sessionStorage.getItem('_user_fio')){
        $("#modal-buy input[name=FIO]").val(sessionStorage.getItem('_user_fio'));
    }
    if (sessionStorage.getItem('_user_phone')) {
        $("#modal-buy input[name=PHONE]").val(sessionStorage.getItem('_user_phone'));
    }

    $("#modal-buy input[name=FIO]")?.suggestions({
        token: "01c012e953fa738ad5bba34557d0bbc9f44102aa",
        type: "NAME",
        deferRequestBy: 400,
        params: {
            parts: ["NAME"]
        },
        /* Вызывается, когда пользователь выбирает одну из подсказок */
        onSelect: function(suggestion) {
            if (suggestion?.data.gender) {
                sessionStorage.setItem('_user_gender', suggestion.data.gender.toLowerCase());
            }
        }
    });

});

function vkLead(){
    let currentCounter = localStorage.getItem('_vkLead');
    console.log('_vkLead');
    if (!currentCounter) {
        _tmr.push({ type: 'reachGoal', id: 3284847, goal: 'lead'});
        console.log('_vkLead2');
        localStorage.setItem('_vkLead', 1);
        return;
    }
    return;
}

