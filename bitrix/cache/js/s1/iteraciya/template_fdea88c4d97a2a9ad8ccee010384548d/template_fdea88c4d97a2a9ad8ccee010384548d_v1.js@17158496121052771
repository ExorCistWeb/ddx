    title	: {
    type: 'outside'
    },
    thumbs	: {
    width	: 50,
    height	: 50
    },
    },
    tpl: {
    closeBtn: '<button class="callback__close-button" type="button" aria-label="Закрыть окно"></button>',
    next: '<a title="Next" class="fancybox-nav fancybox-next" href="javascript:;"><img src="/local/templates/iteraciya/img/fb-right.png"/></a>',
    prev: '<a title="Previous" class="fancybox-nav fancybox-prev" href="javascript:;"><img src="/local/templates/iteraciya/img/fb-left.png"/></a>'
    }
    
    });*/

    /*if ($(".feedback__wrapper2").length) {
    $('.feedback__wrapper--success').fadeIn(300);
    $('.callback__title-wrapper').hide();
    $('.callback__form-wrapper').hide();
    $('.main_feedback__wrapper').hide();
    }*/

    $("#donate_phone").mask('+7 (999) 999-99-99', {placeholder: '+7 ( )'});

//$("#callback-form-main input[name='PHONE']").mask('+7 (999) 999-99-99', {placeholder: 'Телефон*'});

    //$("#callback-form-main input[name='PHONE']").mask('+799999999999', {placeholder: '+7 (___) ___-__-__'});
    //$("#callback-form-buy input[name='PHONE']").mask('+799999999999', {placeholder: '+7 (___) ___-__-__'});

    $("#donate_summ").keyup(function () {
        this.value = this.value.replace(/[^0-9\.]/g, '');
    });

    if ($("#timetable").length) {
        if ($("#timetable .timetable__wrapper").length) {
            $("#timetable").css('opacity', 1);
        } else {
            $("#timetable").hide();
            $(".group-training").addClass("w100");
        }
    }

    $(document).on("click", ".js-callback-button", function () {
        var clubId = parseInt($(this).attr('data-club-id')) || 0;
        $("#callback-form input[name='CLUB_FROM_ID']").val(clubId);
    });
}

function pureUrl(str) {

    var result = str.replace('?', '&');
    result = decodeURIComponent(result).split('&');

    return result[0];

}

function unserialize(str) {

    var tmp;
    var result = str.replace('?', '&');
    result = decodeURIComponent(result).split('&');

    let arResult = {};

    for (var i = 0; i < result.length; i++) {

        if (result[i].indexOf('=') == -1) {
//arResult['PURE_URL'] = result[i];
        } else {
            tmp = result[i].split('=');
            arResult[tmp[0]] = tmp[1];
        }

    }

    return arResult;

}

function isValidEmail(email) {
    var pattern = /^([a-z0-9_\.-])+@[a-z0-9-]+\.([a-z]{2,4}\.)?[a-z]{2,4}$/i;
    if (pattern.test(email)) {
        return true;
    } else {
        return false;
    }
}

function isValidPhone(phone) {
    var pattern = /^[0-9()+ -]{1,99}$/;
    if (pattern.test(phone)) {
        return true;
    } else {
        return false;
    }
}

function number_format(number, decimals, dec_point, thousands_sep) { //number_format(1000, 0, '', ' ');
    number = (number + '').replace(/[^0-9+\-Ee.]/g, '');
    var n = !isFinite(+number) ? 0 : +number,
        prec = !isFinite(+decimals) ? 0 : Math.abs(decimals),
        sep = (typeof thousands_sep === 'undefined') ? ',' : thousands_sep,
        dec = (typeof dec_point === 'undefined') ? '.' : dec_point,
        s = '',
        toFixedFix = function (n, prec) {
            var k = Math.pow(10, prec);
            return '' + (Math.round(n * k) / k)
                .toFixed(prec);
        };
// Fix for IE parseFloat(0.55).toFixed(0) = 0;
    s = (prec ? toFixedFix(n, prec) : '' + Math.round(n))
        .split('.');
    if (s[0].length > 3) {
        s[0] = s[0].replace(/\B(?=(?:\d{3})+(?!\d))/g, sep);
    }
    if ((s[1] || '')
        .length < prec) {
        s[1] = s[1] || '';
        s[1] += new Array(prec - s[1].length + 1)
            .join('0');
    }
    return s.join(dec);
}

function onScrollToObj(scroll_el) {
    if ($(scroll_el).length != 0) {
        $('html, body').animate({scrollTop: $(scroll_el).offset().top}, 500);
    }
}

function onClickSeeMore(url) {
    showWaiting();
    $.get(url, function (data) {
        var html = $(data).find('.ajax_replace').html();
        $('.ajax_replace').html(html);

        initJS();
        hideWaiting();

    });
}

