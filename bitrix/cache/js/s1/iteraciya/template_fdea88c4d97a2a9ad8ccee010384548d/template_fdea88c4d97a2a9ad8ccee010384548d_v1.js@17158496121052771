'use strict';

$(document).ready(function () {

  // Получаем нужный элемент
  var mainSliderBlock = document.querySelector('.slider-main');
  var headerBlock = document.querySelector('.page-header');
  var advantagesBlock = document.querySelector('.advantages');
  var maintitleBlock = document.querySelector('.page-main__title');
  var currentPlansSlide = document.querySelector('.plans__item-full');
  var plansTitle =document.querySelector('.plans__title');
  var mobileInactiveSlides = document.querySelectorAll('.js-view-animation-desktop');
  var feedbackBlock = document.querySelector('.feedback');
  var footerBlock = document.querySelector('.page-footer');
  var imageSlider = document.querySelector('.image-slider');
  var coachesItems = document.querySelectorAll('.coaches__item');
  var timetableBlock = document.querySelector('.timetable');
  var groupTrainingBlock = document.querySelector('.group-training');
  var videoBlocks = document.querySelectorAll('.video');
  var disciplineBlock = document.querySelector('.discipline');
  var realEastateDescrBlock = document.querySelector('.real-estate__description');
  var cloudDescrBlock = document.querySelector('.cloud__description');
  var braceletFeateaInemBlocks = document.querySelectorAll('.bracelet__features-item');
  var whanWeHaveItemBlocks = document.querySelectorAll('.what-we-have__item');
  var careerBlock = document.querySelector('.career');
  var vacancyLocationBlock = document.querySelector('.vacancy__location');
  var vacancyExpBlock = document.querySelector('.vacancy__experience');
  var vacancyScheluleBlock = document.querySelector('.vacancy__schedule');
  var vacancyTextContentTitles = document.querySelectorAll('.vacancy__text-content h3');
  var vacancyTextContentItems = document.querySelectorAll('.vacancy__text-content li');
  var partnershipDescrBlock = document.querySelector('.partnership__description');
  var errorPage = document.querySelector('.page-404');
  var addressItemBlocks = document.querySelectorAll('.address__item');
  var coachesNoteListBlock = document.querySelector('.coaches__note-list');
  var clubAmentiesBlock = document.querySelector('.club__amenities');
  var coachesPreviewBlock = document.querySelector('.coaches-preview');
  var clubCloudBlock = document.querySelector('.club__cloud');
  var clubContactsBlock = document.querySelector('.club__contacts');
  var clubNewsBlock = document.querySelector('.club__news');
  var clubPlansBlock = document.querySelector('.club__plans');
  var shutters = document.querySelectorAll('.shutter');
  var faqItems = document.querySelectorAll('.faq li');
  var faqAnswers = document.querySelectorAll('.faq__answer');
  var faqTextContentTitles = document.querySelectorAll('.faq__question h3');
  var faqTextContentItems = document.querySelectorAll('.faq__answer p');
  var faqTextContentItemsOpen = document.querySelectorAll('.faq li.is-open .faq__answer p');

  var visible = function (target, animationClass, viewtop, viewbottom) {
    // Все позиции элемента
    var targetPosition = {
      top: window.pageYOffset + target.getBoundingClientRect().top,
      left: window.pageXOffset + target.getBoundingClientRect().left,
      right: window.pageXOffset + target.getBoundingClientRect().right,
      bottom: window.pageYOffset + target.getBoundingClientRect().bottom
    };
      // Получаем позиции окна
    var windowPosition = {
      top: window.pageYOffset,
      left: window.pageXOffset,
      right: window.pageXOffset + document.documentElement.clientWidth,
      bottom: window.pageYOffset + document.documentElement.clientHeight
    };

    if (targetPosition.bottom > (windowPosition.top * viewtop) && // Если позиция нижней части элемента больше позиции верхней чайти окна, то элемент виден сверху
      targetPosition.top < windowPosition.bottom * viewbottom && // Если позиция верхней части элемента меньше позиции нижней чайти окна, то элемент виден снизу
      targetPosition.right > windowPosition.left && // Если позиция правой стороны элемента больше позиции левой части окна, то элемент виден слева
      targetPosition.left < windowPosition.right) { // Если позиция левой стороны элемента меньше позиции правой чайти окна, то элемент виден справа
      // Если элемент полностью видно, то запускаем следующий код

      if (!target.classList.contains(animationClass)) {
        target.classList.add(animationClass);
      }
    } else {
      // Если элемент не видно, то запускаем этот код
    }
  };

  // Запускаем функцию при прокрутке страницы

  window.addEventListener('scroll', function () {
    if (headerBlock) {
      visible(headerBlock, 'js-animation', 1, 1);
    }
    if (maintitleBlock) {
      visible(maintitleBlock, 'js-animation', 1, 1);
    }
    if (feedbackBlock) {
      visible(feedbackBlock, 'js-animation', 0.7, 1);
    }
    if (footerBlock) {
      visible(footerBlock, 'js-animation', 0.7, 1);
    }
    if (plansTitle) {
      visible(plansTitle, 'js-animation', 0.7, 1);
    }
    if (coachesItems) {
      coachesItems.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 0.9);
      });
    }
    if (imageSlider) {
      visible(imageSlider, 'js-animation', 0.7, 1);
    }
    if (currentPlansSlide) {
      visible(currentPlansSlide, 'js-animation', 0.2, 0.8);
    }
    if (mobileInactiveSlides) {
      mobileInactiveSlides.forEach(function (element) {
        visible(element, 'js-animation-desktop', 0.7, 0.8);
      });
    }
    if (mainSliderBlock) {
      visible(mainSliderBlock, 'js-animation', 1, 1);
    }
    if (advantagesBlock) {
      visible(advantagesBlock, 'js-animation', 0.7, 0.9);
    }
    if (timetableBlock) {
      visible(timetableBlock, 'js-animation', 0.7, 1);
    }
    if (groupTrainingBlock) {
      visible(groupTrainingBlock, 'js-animation', 0.7, 1);
    }
    if (videoBlocks) {
      videoBlocks.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (disciplineBlock) {
      visible(disciplineBlock, 'js-animation', 0.7, 1);
    }
    if (realEastateDescrBlock) {
      visible(realEastateDescrBlock, 'js-animation', 1, 1);
    }
    if (cloudDescrBlock) {
      visible(cloudDescrBlock, 'js-animation', 0.7, 1);
    }
    if (braceletFeateaInemBlocks) {
      braceletFeateaInemBlocks.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (whanWeHaveItemBlocks) {
      whanWeHaveItemBlocks.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 0.8);
      });
    }
    if (careerBlock) {
      visible(careerBlock, 'js-animation', 0.7, 1);
    }
    if (vacancyLocationBlock && vacancyExpBlock && vacancyScheluleBlock) {
      visible(vacancyLocationBlock, 'js-animation', 0.7, 1);
      visible(vacancyExpBlock, 'js-animation', 0.7, 1);
      visible(vacancyScheluleBlock, 'js-animation', 0.7, 1);
    }
    if (vacancyTextContentTitles) {
      vacancyTextContentTitles.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (vacancyTextContentTitles) {
      vacancyTextContentTitles.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (vacancyTextContentItems) {
      vacancyTextContentItems.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (partnershipDescrBlock) {
      visible(partnershipDescrBlock, 'js-animation', 0.7, 1);
    }
    if (errorPage) {
      visible(errorPage, 'js-animation', 0.7, 1);
    }
    /*if (addressItemBlocks) {
      addressItemBlocks.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }*/
    if (coachesNoteListBlock) {
      visible(coachesNoteListBlock, 'js-animation', 1, 1);
    }
    if (clubAmentiesBlock) {
      visible(clubAmentiesBlock, 'js-animation', 0.7, 1);
    }
    if (coachesPreviewBlock) {
      visible(coachesPreviewBlock, 'js-animation', 0.7, 0.9);
    }
    if (clubCloudBlock) {
      visible(clubCloudBlock, 'js-animation', 0.7, 1);
    }
    if (clubContactsBlock) {
      visible(clubContactsBlock, 'js-animation', 0.7, 1);
    }
    if (clubNewsBlock) {
      visible(clubNewsBlock, 'js-animation', 0.7, 1);
    }
    if (clubPlansBlock) {
      visible(clubPlansBlock, 'js-animation', 0.7, 1);
    }
    shutters.forEach(function (shutter) {
      visible(shutter, 'js-animation', 0.7, 1);
    });
    if (faqItems) {
      faqItems.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (faqAnswers) {
      faqAnswers.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (faqTextContentTitles) {
      faqTextContentTitles.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (faqTextContentItems) {
      faqTextContentItems.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
    if (faqTextContentItemsOpen) {
      faqTextContentItemsOpen.forEach(function (element) {
        visible(element, 'js-animation', 0.7, 1);
      });
    }
  });

  // А также запустим функцию сразу. А то вдруг, элемент изначально видно
  if (headerBlock) {
    visible(headerBlock, 'js-animation', 1, 1);
  }
  if (maintitleBlock) {
    visible(maintitleBlock, 'js-animation', 1, 1);
  }
  if (feedbackBlock) {
    visible(feedbackBlock, 'js-animation', 0.7, 1);
  }
  if (footerBlock) {
    visible(footerBlock, 'js-animation', 0.7, 1);
  }
  if (coachesItems) {
    coachesItems.forEach(function (element) {
      visible(element, 'js-animation', 0.7, 0.9);
    });
  }
  if (imageSlider) {
    visible(imageSlider, 'js-animation', 0.7, 1);
  }
  if (currentPlansSlide) {
    visible(currentPlansSlide, 'js-animation', 0.2, 0.8);
  }
  if (mobileInactiveSlides) {
    mobileInactiveSlides.forEach(function (element) {
      visible(element, 'js-animation-desktop', 0.7, 0.8);
    });
  }
  if (plansTitle) {
    visible(plansTitle, 'js-animation', 0.7, 1);
  }
  if (advantagesBlock) {
    visible(advantagesBlock, 'js-animation', 0.7, 0.9);
  }
  if (mainSliderBlock) {
    visible(mainSliderBlock, 'js-animation', 1, 1);
  }
  if (timetableBlock) {
    visible(timetableBlock, 'js-animation', 0.7, 1);
  }
  if (groupTrainingBlock) {
    visible(groupTrainingBlock, 'js-animation', 0.7, 1);
  }
  if (videoBlocks) {
    videoBlocks.forEach(function (element) {
      visible(element, 'js-animation', 0.7, 1);
    });
  }
  if (disciplineBlock) {
    visible(disciplineBlock, 'js-animation', 0.7, 1);
  }
  if (realEastateDescrBlock) {
    visible(realEastateDescrBlock, 'js-animation', 1, 1);
  }
  if (cloudDescrBlock) {
    visible(cloudDescrBlock, 'js-animation', 0.7, 1);
  }
  if (braceletFeateaInemBlocks) {
    braceletFeateaInemBlocks.forEach(function (element) {
      visible(element, 'js-animation', 0.7, 1);
    });
  }
  if (whanWeHaveItemBlocks) {
    whanWeHaveItemBlocks.forEach(function (element) {
      visible(element, 'js-animation', 0.7, 0.8);
    });
  }
  if (careerBlock) {
    visible(careerBlock, 'js-animation', 0.7, 1);
  }
  if (vacancyLocationBlock && vacancyExpBlock && vacancyScheluleBlock) {
    visible(vacancyLocationBlock, 'js-animation', 0.7, 1);
    visible(vacancyExpBlock, 'js-animation', 0.7, 1);
    visible(vacancyScheluleBlock, 'js-animation', 0.7, 1);
  }
  if (vacancyTextContentTitles) {
    vacancyTextContentTitles.forEach(function (element) {
      visible(element, 'js-animation', 0.7, 1);
    });
  }
  if (vacancyTextContentItems) {
    vacancyTextContentItems.forEach(function (element) {
