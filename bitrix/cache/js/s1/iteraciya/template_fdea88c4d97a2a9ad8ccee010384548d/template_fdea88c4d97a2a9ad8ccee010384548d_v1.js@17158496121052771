BX(function () {
    var className = 'jscolor';

    var installJsColor = function (className) {
        if (document.getElementsByClassName(className).length > 0) {
            jscolor.installByClassName(className);
        }
    };

    BX.addCustomEvent('onFixedNodeChangeState', function () {
        installJsColor(className);
        multyJsColorHandler();
    });
    BX.addCustomEvent('onAjaxSuccess', function () {
        installJsColor(className);
    });
    BX.addCustomEvent('onAdminTabsChange', function () {
        installJsColor(className);
        multyJsColorHandler();
    });

    var multyJsColorHandler = function () {
        document.querySelector('body').onclick = function(event) {
            var target = event.target;

            if(target.matches('input[type="button"]')) {

                var parentRow = target.parentNode.parentNode;

                var previousRow = parentRow.previousSibling;

                var jsColorInput = previousRow.firstChild.firstChild;

                if(!jsColorInput.matches('input[type="text"].jscolor')) {
                    return;
                }

                installJsColor(className);
            }

        };
    }
});
/* End */
;
; /* Start:"a:4:{s:4:"full";s:85:"/local/templates/iteraciya/components/bitrix/news.list/forms/script.js?16971963483388";s:6:"source";s:70:"/local/templates/iteraciya/components/bitrix/news.list/forms/script.js";s:3:"min";s:0:"";s:3:"map";s:0:"";}"*/
$(document).ready(function () {
    var titleResume = (arParams.TITLE_RESUME?.length > 0) ? arParams.TITLE_RESUME : 'Прикрепить резюме';
    if ($('#fileField').length > 0){
        $('#fileField').filer({
            limit: 1,
            maxSize: 10,
            fileMaxSize: 10,
            extensions: ['pdf', 'doc', 'docx'],
            showThumbs: true,
            addMore: true,
            allowDuplicates: false,
            changeInput: `<div class="box__input">
                            <span class="title">${titleResume}</span>
                            <span class="description">Перетащите файл сюда или <strong>загрузите файл</strong></span>
                            <span class="subdescription">Размер файла не должен превышать 10 Мб</br>
                            Поддерживаемый формат файлов: pdf, doc, docx</span>
                        </div>
            `,
            templates: {
                box: '<div class="box__input__file__list"></div>',
                item: `
                    <div class="box__success">
                        <svg width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
                            <circle cx="10" cy="10" r="9" stroke="#0CA1B1" stroke-width="2"/>
                            <path d="M6 10L9 13L14 7" stroke="#0CA1B1" stroke-width="2" stroke-linecap="round"/>
                        </svg>
                        <span>{{fi-name}}</span>
                        <div class="btn__trash"></div>
                    </div>
                `,
                _selectors: {
                    list: '.box__input__file__list',
                    item: '.box__success',
                    remove: '.btn__trash'
                }
            },
            dragDrop: {},
            captions: {
                button: "Выбрать файлы",
                feedback: "Выберете файлы для загрузки",
                feedback2: "файлов для загрузки",
                removeConfirmation: "Вы уверены что хотите удалить файл?",
                errors: {
                    filesLimit: "Количество загружаемых файлов не должно превышать 1шт.",
                    filesType: "Используйте только файлы формата pdf, doc, docx",
                    filesSizeAll: "Максимальный размер загружаемых файлов должен быть не более {{fi-maxSize}} МБ",
                    filesSize: "{{fi-name}} слишком большой. Максимальный размер файла {{fi-fileMaxSize}} МБ"
                }
            },
            onSelect: function(e) {
                document.getElementById('fileField').parentNode.classList.remove('callback__text-field--error', 'callback__text-field--error_undefined');
                document.querySelector('.file__input__box .box__input').classList.add('is-upload');
            },
            onEmpty: function(e) {
                document.querySelector('.file__input__box .box__input').classList.remove('is-upload');
            }
        });
    }
    
});
